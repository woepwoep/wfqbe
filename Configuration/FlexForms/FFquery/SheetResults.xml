<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:sheet_results</sheetTitle>
			<sheetDescription>Dit is de omschrijving van de flexform</sheetDescription>
			<sheetShortDescr>Dit is een korte omschrijving in de flexform</sheetShortDescr>
        </TCEforms>
        <type>array</type>
        <el>
            <recordsForPage>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:max_items_in_list</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <max>3</max>
                        <eval>required,trim,num,int</eval>
                        <range>
                            <lower>1</lower>
                        </range>
                        <default>25</default>
                    </config>
                </TCEforms>
            </recordsForPage>
            <sortField>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:sortField</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </sortField>
            <emptyResult>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:emptyResult</label>
                    <config>
                        <type>text</type>
                        <size>50</size>
                        <max>50</max>
						<enableRichtext>1</enableRichtext>
                    </config>
                </TCEforms>
            </emptyResult>
            <csvDownload>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:csvDownload</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </csvDownload>
        </el>
    </ROOT>
</T3DataStructure>
