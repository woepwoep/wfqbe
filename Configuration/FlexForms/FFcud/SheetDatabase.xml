<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:sheet_database</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <targetTable>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:targetTable</label>
					<onChange>reload</onChange>
                    <config>
                        <type>select</type>
                        <renderType>selectSingle</renderType>
						<itemsProcFunc>RedSeadog\Wfqbe\UserFunc\Field->populateTargetTable</itemsProcFunc>
                        <minitems>0</minitems>
                        <maxitems>1</maxitems>
                        <size>1</size>
                    </config>
                
            </targetTable>
            <identifiers>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:identifiers</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>45</max>
                        <eval>trim</eval>
                    </config>
            </identifiers>
            <fieldlist>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:fieldlist</label>
					<onChange>reload</onChange>
                    <config>
                        <type>select</type>
                        <renderType>selectMultipleSideBySide</renderType>
						<enableMultiSelectFilterTextfield>true</enableMultiSelectFilterTextfield>
                        <itemsProcFunc>RedSeadog\Wfqbe\UserFunc\Field->getColumnNames</itemsProcFunc>
                        <minitems>0</minitems>
                        <maxitems>45</maxitems>
                        <size>5</size>
                    </config>
            </fieldlist>
			<fieldtypes>
                <section>1</section>
                    <type>array</type>
                    <title>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:fieldtypes</title>
                    <el>
                        <typesection>
                            <type>array</type>
                            <el>
                                <name>
					<TCEforms>
						<label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:name</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<itemsProcFunc>RedSeadog\Wfqbe\UserFunc\Field->populateFieldNames</itemsProcFunc>
                        				<minitems>0</minitems>
                        				<maxitems>1</maxitems>
                        				<size>1</size>
						</config>
					</TCEforms>
				</name>
                                <veldtype>
					<label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:veldtype</label>
					<onChange>reload</onChange>
					<config>
						<type>select</type>
							<renderType>selectSingle</renderType>
							<itemsProcFunc>RedSeadog\Wfqbe\UserFunc\Field->populateFieldTypes</itemsProcFunc>
							<autosizemax>2</autosizemax>
							<size>25</size>
							<maxitems>1</maxitems>
							<minitems>1</minitems>
							<size>1</size>
					</config>
				</veldtype>
				<datumFormat>
					<label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:date_format</label>
					<displayCond>FIELD:veldtype:=:datum</displayCond>
					<config>
						<type>input</type>
						<cols>15</cols>
						<eval>trim</eval>
					</config>
				</datumFormat>
				<currency>
					<label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:valuta</label>
					<displayCond>FIELD:veldtype:=:valuta</displayCond>
					<config>
						<type>input</type>
						<cols>15</cols>
						<eval>trim</eval>
					</config>
				</currency>
				<custom>
					<label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:custom</label>
					<displayCond>FIELD:veldtype:=:custom</displayCond>
					<config>
						<type>input</type>
						<cols>15</cols>
						<eval>trim</eval>
					</config>
				</custom>
                            </el>
                        </typesection>
                   </el>
            </fieldtypes>
            <defaultvalues>
                
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_cud.xlf:fixedvalues</label>
                    <config>
                        <type>text</type>
                        <cols>45</cols>
                        <rows>5</rows>
                        <eval>trim</eval>
                    </config>
                
            </defaultvalues>
        </el>
    </ROOT>
</T3DataStructure>
